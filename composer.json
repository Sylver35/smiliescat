{
    "name": "sylver35/smiliescat",
    "type": "phpbb-extension",
    "description": "This extension add categories for the smilies.",
    "homepage": "https://breizhcode.com",
    "version": "1.7.0",
    "time": "2024-12-01",
    "keywords": [
        "phpbb",
        "extension",
        "breizh",
        "smiley",
        "smilies",
        "categories",
        "posting"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Sylver35",
            "homepage": "https://breizhcode.com",
            "email": "webmaster@breizhcode.com",
            "role": "Extension Developer"
        },
        {
            "name": "Skouat",
            "homepage": "https://github.com/Skouat",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "~2.2.0",
        "phpbb/phpbb": ">=3.3.13"
    },
	 "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "display-name": "Breizh Smilies Categories",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.13"
        },
        "version-check": {
            "host": "breizhversions.com",
            "directory": "/smiliescat",
            "filename": "version_check.json",
            "ssl": false
        }
    }
}
